{
	"name": "spaanproductions/laravel-carbon-holidays",
	"description": "Add Dutch Holidays Check to Carbon in Laravel",
	"keywords": [
		"spaanproductions",
		"laravel-carbon-holidays"
	],
	"homepage": "https://github.com/spaanproductions/laravel-carbon-holidays",
	"license": "MIT",
	"type": "library",
	"authors": [
		{
			"name": "Spaan Productions",
			"email": "info@spaanproductions.nl",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^8.0|^8.1",
		"illuminate/support": "^9.0",
		"nesbot/carbon": "^2.0",
		"ext-calendar": "*"
	},
	"require-dev": {
		"orchestra/testbench": "^7.0",
		"phpunit/phpunit": "^9.0"
	},
	"autoload": {
		"psr-4": {
			"SpaanProductions\\LaravelCarbonHolidays\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"SpaanProductions\\LaravelCarbonHolidays\\Tests\\": "tests"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit",
		"test-coverage": "vendor/bin/phpunit --coverage-html coverage"
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"providers": [
				"SpaanProductions\\LaravelCarbonHolidays\\LaravelCarbonHolidaysServiceProvider"
			],
			"aliases": {
				"LaravelCarbonHolidays": "SpaanProductions\\LaravelCarbonHolidays\\LaravelCarbonHolidaysFacade"
			}
		}
	}
}
